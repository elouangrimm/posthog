{
  "rewrites": [
    {
      "source": "/e/:path(.*)",
      "destination": "https://us.i.posthog.com/e/:path"
    },
    {
      "source": "/i/:path(.*)",
      "destination": "https://us.i.posthog.com/i/:path"
    },
    {
      "source": "/static/:path(.*)",
      "destination": "https://us.i.posthog.com/static/:path"
    },
    {
      "source": "/decide/:path(.*)",
      "destination": "https://us.i.posthog.com/decide/:path"
    },
    {
      "source": "/array/static/:path(.*)",
      "destination": "https://us-assets.i.posthog.com/static/:path"
    },
     {
      "source": "/array/:path(.*)",
      "destination": "https://us.i.posthog.com/array/:path"
    }
  ],
  "headers": [
    {
      "source": "/e/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://www.elouan.xyz" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
      ]
    },
     {
      "source": "/decide/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://www.elouan.xyz" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
      ]
    },
     {
      "source": "/array/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://www.elouan.xyz" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
      ]
    }
  ]
}
